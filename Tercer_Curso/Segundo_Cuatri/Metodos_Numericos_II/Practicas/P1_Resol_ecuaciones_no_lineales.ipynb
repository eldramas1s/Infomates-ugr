    "%%writefile busqueda_incremental.py\n",
    "# -*- coding: UTF-8 -*-\n",
    "## modulo busqueda_incremental\n",
    "'''\n",
    "x1,x2 = busqueda_incremental(f,a,b,dx)\n",
    "buscar谩 dentro del intervalo [a,b] en incrementos de dx una acotaci贸n\n",
    "de la forma [x1,x2] de la ra铆z m谩s peque帽a de f(x) en dicho intervalo.\n",
    "Se devolver谩 x1 = x2 = None en caso de no encontrarse ninguna.\n",
    "'''\n",
    "# global f,a,b,dx\n",
    "\n",
    "def busqueda_incremental(f,a,b,dx):\n",
    "    x1, x2 = a, a + dx\n",
    "    f1, f2 = f(x1), f(x2)\n",
    "    while np.sign(f1) == np.sign(f2):\n",
    "        if x1 >= b:\n",
    "            return None,None   # se podr铆a cambiar por x2 >= b\n",
    "        x1, x2 = x2, x2 + dx\n",
    "        f1, f2 = f2, f(x2)\n",
    "    else: # si no se cumple la condici贸n del while\n",
    "        return x1,x2"
