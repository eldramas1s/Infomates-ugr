    <RepositoryURL>https://github.com/xonotic/xonotic</RepositoryURL>
    <Maintainer>Michael Larabel</Maintainer>
  </TestProfile>
  <TestSettings>
    <Default>
      <Arguments>-nohome -benchmark demos/the-big-keybench +r_glsl 1 +vid_desktopfullscreen 0 +cl_curl_enabled 0 +r_texture_dds_load 1 +cl_playerdetailreduction 0 +developer 1 +cl_allow_uid2name 0</Arguments>
