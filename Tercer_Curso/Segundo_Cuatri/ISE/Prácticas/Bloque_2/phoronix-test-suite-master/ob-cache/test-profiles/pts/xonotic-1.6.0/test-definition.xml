<?xml version="1.0"?>
<!--Phoronix Test Suite v10.8.3-->
<PhoronixTestSuite>
  <TestInformation>
    <Title>Xonotic</Title>
    <AppVersion>0.8.5</AppVersion>
    <Description>This is a benchmark of Xonotic, which is a fork of the DarkPlaces-based Nexuiz game. Development began in March of 2010 on the Xonotic game.</Description>
    <ResultScale>Frames Per Second</ResultScale>
    <Proportion>HIB</Proportion>
    <TimesToRun>3</TimesToRun>
