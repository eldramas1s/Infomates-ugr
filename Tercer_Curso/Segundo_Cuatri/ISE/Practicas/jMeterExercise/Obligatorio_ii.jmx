          <stringProp name="TestPlan.comments">Tiempo de espera</stringProp>
        </ConstantTimer>
        <hashTree/>
        <HTTPSamplerProxy guiclass="HttpTestSampleGui" testclass="HTTPSamplerProxy" testname="Recuperar Datos alumno" enabled="true">
