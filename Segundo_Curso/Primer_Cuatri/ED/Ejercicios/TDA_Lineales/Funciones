#include <iostream>
#include <algorithm>
#include <list>
#include <queue>
#include <stack>

using namespace std;

void dividirlista(list <int> lista, int k){

    list <int>::iterator it = list.begin();

    while ( *it != list.end()){
        if(*it>k){
            list.puch_back(*it);
            it=list.erase(it);
        }
        else
            ++it;
    }
}

